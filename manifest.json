{
	"manifest_version": 2,

	"name": "Entry Point Regulator Prototype",
	"author": "David Ross",
	"description": "Prototype implementation of Entry Point Regulation (EPR)",
        "short_name": "EPR Prototype",
	"version": "0.54",

	"background": {
		"scripts": ["background.js"]
	},

	"permissions": [ "webRequest", "webRequestBlocking", "bookmarks", "unlimitedStorage", "*://*/*" ],
	"browser_action": { 
	  "default_icon": "icon.png",
	  "default_popup": "popup.html"
	}
}
